<h1>Listing project_user_roles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Role</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_user_roles.each do |project_user_role| %>
      <tr>
        <td><%= project_user_role.user_id %></td>
        <td><%= project_user_role.role_id %></td>
        <td><%= project_user_role.project_id %></td>
        <td><%= link_to 'Show', project_user_role %></td>
        <td><%= link_to 'Edit', edit_project_user_role_path(project_user_role) %></td>
        <td><%= link_to 'Destroy', project_user_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project user role', new_project_user_role_path %>
